<h1 class='index-title'>New User</h1>

<div class='form'>
  <%= form_for @user do |f| %>
    <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
    </div>
    <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
    </div>
    <div>
    <%= f.submit "Submit" %>
    </div>
  <% end %>
</div>
