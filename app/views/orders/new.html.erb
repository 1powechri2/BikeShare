<h1 class='index-title'>Enter Order Info</h1>

<div class='form'>
  <%= form_for [current_user, @order] do |f| %>
    <div>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    </div>
    <div>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    </div>
    <div>
    <%= f.label :street %>
    <%= f.text_field :street %>
    </div>
    <div>
    <%= f.label :city %>
    <%= f.text_field :city %>
    </div>
    <div>
    <%= f.label :state %>
    <%= f.text_field :state %>
    </div>
    <div>
    <%= f.label :zip %>
    <%= f.number_field :zip %>
    </div>
    <div>
    <%= f.submit %>
    </div>
  <% end %>
</div>
