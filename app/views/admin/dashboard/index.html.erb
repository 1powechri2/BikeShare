<nav>
  <% if current_admin? %>
    <p>Logged in as Admin User: <%= current_user.username %></p>
    <%= link_to 'Logout', logout_path, method: :delete %>
  <% elsif current_user %>
    <p>Logged in as <%= current_user.username %></p>
    <%= link_to 'Logout', logout_path, method: :delete %>
  <% end %>
</nav>
